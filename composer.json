{
    "name": "brainsum/drupal-training",
    "description": "Project for helping users getting familiar with Drupal.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/address": "^1.8",
        "drupal/addtoany": "^1.14",
        "drupal/admin_toolbar": "^2.2",
        "drupal/bootstrap": "^3.23",
        "drupal/ckeditor_video": "^1.0",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.9",
        "drupal/corporate_blue": "^1.1",
        "drupal/devel": "^2.1",
        "drupal/entity_type_clone": "^1.3",
        "drupal/fakeobjects": "^1.0",
        "drupal/field_group": "^3.1",
        "drupal/geolocation": "^3.2",
        "drupal/mailchimp": "^1.11",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/olivero": "^1.0",
        "drupal/paragraphs": "^1.12",
        "drupal/redis": "^1.4",
        "drupal/taxonomy_access_fix": "^2.7",
        "drush/drush": "^10.2",
        "platformsh/config-reader": "^2.3",
        "symfony/dotenv": "^5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
